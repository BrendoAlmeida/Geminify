{
  "name": "geminify",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "node --loader ts-node/esm src/server.ts",
    "dev": "npx nodemon --watch src --ext ts,json --exec \"node --loader ts-node/esm\" src/server.ts",
    "build": "npm run fix-imports && tsc",
    "build:prod": "npm run build && npm run copy-static",
    "copy-static": "node scripts/copy-files.js",
    "fix-imports": "node scripts/fix-imports.js",
    "serve": "node dist/server.js",
    "serve:prod": "cross-env NODE_ENV=production node dist/server.js",
    "setup": "node --loader ts-node/esm scripts/setup.ts",
    "postinstall": "npm run build:prod",
    "clean": "node scripts/clean.js",
    "deploy": "npm run clean && npm run build:prod && npm run serve:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.12.0",
    "@types/express-session": "^1.18.2",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "formidable": "^3.5.4",
    "helmet": "^8.1.0",
    "prettier": "^3.3.2",
    "spotify-preview-finder": "^2.1.0",
    "spotify-web-api-node": "^5.0.2",
    "superagent": "^10.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/spotify-web-api-node": "^5.0.11",
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
